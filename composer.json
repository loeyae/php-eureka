{
  "name": "loeyae/php-eureka",
  "description": "php eureka client demo",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "ZhangYi",
      "email": "loeyae@gmail.com"
    }
  ],
  "type": "library",
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "loeye\\": "src/"
    }
